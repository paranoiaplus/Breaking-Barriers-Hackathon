<form action="http://localhost:1337/registerPatient" method="POST">
  <h1>Patient Registration</h1>
    <label for="fullName">Full Name:</label><input type="text" placeholder="John Doe" name="fullName"> <br>
    <label for="phoneNumber">Phone Number:</label><input type="text" placeholder="555-555-555" name="phoneNumber"> <br>
    <label for="emailAddress">Email Address:</label><input type="text" placeholder="foo@bar.baz" name="emailAddress"> <br>
    <input type="submit">
</form>


